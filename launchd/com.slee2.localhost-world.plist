<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.slee2.localhost-world</string>

    <key>ProgramArguments</key>
    <array>
        <string>/etc/profiles/per-user/slee2/bin/bun</string>
        <string>run</string>
        <string>/Users/slee2/projects/localhost-world/src/index.ts</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/slee2/Library/Logs/localhost-world.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/slee2/Library/Logs/localhost-world.error.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>HOME</key>
        <string>/Users/slee2</string>
        <key>PATH</key>
        <string>/etc/profiles/per-user/slee2/bin:/run/current-system/sw/bin:/usr/sbin:/usr/bin:/bin:/sbin</string>
        <key>LANG</key>
        <string>en_US.UTF-8</string>
        <key>LC_ALL</key>
        <string>en_US.UTF-8</string>
    </dict>
</dict>
</plist>
